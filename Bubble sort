
vetor=[4,3,2,1]
trocou = 1

aux=[]
print(vetor)
tam=len(vetor)
while trocou==1:
    trocou=0
    
    i=0
    while i != (tam-1):
       
        if vetor[i]> vetor[i+1]:
            aux=vetor[i]
            vetor[i]=vetor[i+1]
            vetor[i+1]=aux
            print(vetor)
            trocou =1
            
        i=i+1
       
